{
  "1": "اِذَا وَقَعَتِ الۡوَاقِعَةُۙ",
  "2": "لَيۡسَ لِـوَقۡعَتِهَا كَاذِبَةٌ​ۘ",
  "3": "خَافِضَةٌ رَّافِعَةٌۙ",
  "4": "اِذَا رُجَّتِ الۡاَرۡضُ رَجًّا ۙ",
  "5": "وَّبُسَّتِ الۡجِبَالُ بَسًّا ۙ",
  "6": "فَكَانَتۡ هَبَآءً مُّنۡۢبَـثًّاۙ",
  "7": "وَّكُنۡـتُمۡ اَزۡوَاجًا ثَلٰـثَـةًؕ",
  "8": "فَاَصۡحٰبُ الۡمَيۡمَنَةِۙ مَاۤ اَصۡحٰبُ الۡمَيۡمَنَةِ ؕ",
  "9": "وَاَصۡحٰبُ الۡمَشۡـَٔـمَةِۙ مَاۤ اَصۡحٰبُ الۡمَشۡـَٔـمَةِؕ",
  "10": "وَالسّٰبِقُوۡنَ السّٰبِقُوۡنَۚ ۙ",
  "11": "اُولٰٓٮِٕكَ الۡمُقَرَّبُوۡنَ​ۚ",
  "12": "فِىۡ جَنّٰتِ النَّعِيۡمِ",
  "13": "ثُلَّةٌ مِّنَ الۡاَوَّلِيۡنَۙ",
  "14": "وَقَلِيۡلٌ مِّنَ الۡاٰخِرِيۡنَؕ",
  "15": "عَلٰى سُرُرٍ مَّوۡضُوۡنَةٍۙ",
  "16": "مُّتَّكِـــِٕيۡنَ عَلَيۡهَا مُتَقٰبِلِيۡنَ",
  "17": "يَطُوۡفُ عَلَيۡهِمۡ وِلۡدَانٌ مُّخَلَّدُوۡنَۙ",
  "18": "بِاَكۡوَابٍ وَّاَبَارِيۡقَۙ وَكَاۡسٍ مِّنۡ مَّعِيۡنٍۙ",
  "19": "لَّا يُصَدَّعُوۡنَ عَنۡهَا وَلَا يُنۡزِفُوۡنَۙ",
  "20": "وَفَاكِهَةٍ مِّمَّا يَتَخَيَّرُوۡنَۙ",
  "21": "وَلَحۡمِ طَيۡرٍ مِّمَّا يَشۡتَهُوۡنَؕ",
  "22": "وَحُوۡرٌ عِيۡنٌۙ",
  "23": "كَاَمۡثَالِ اللُّـؤۡلُـوٴِالۡمَكۡنُوۡنِ​ۚ",
  "24": "جَزَآءًۢ بِمَا كَانُوۡا يَعۡمَلُوۡنَ",
  "25": "لَا يَسۡمَعُوۡنَ فِيۡهَا لَغۡوًا وَّلَا تَاۡثِيۡمًا ۙ",
  "26": "اِلَّا قِيۡلًا سَلٰمًا سَلٰمًا",
  "27": "وَاَصۡحٰبُ الۡيَمِيۡنِۙ مَاۤ اَصۡحٰبُ الۡيَمِيۡنِؕ",
  "28": "فِىۡ سِدۡرٍ مَّخۡضُوۡدٍۙ",
  "29": "وَّطَلۡحٍ مَّنۡضُوۡدٍۙ",
  "30": "وَّظِلٍّ مَّمۡدُوۡدٍۙ",
  "31": "وَّ مَآءٍ مَّسۡكُوۡبٍۙ",
  "32": "وَّفَاكِهَةٍ كَثِيۡرَةٍۙ",
  "33": "لَّا مَقۡطُوۡعَةٍ وَّلَا مَمۡنُوۡعَةٍۙ",
  "34": "وَّ فُرُشٍ مَّرۡفُوۡعَةٍؕ",
  "35": "اِنَّاۤ اَنۡشَاۡنٰهُنَّ اِنۡشَآءًۙ",
  "36": "فَجَعَلۡنٰهُنَّ اَبۡكَارًاۙ",
  "37": "عُرُبًا اَتۡرَابًاۙ",
  "38": "لِّاَصۡحٰبِ الۡيَمِيۡنِؕ",
  "39": "ثُلَّةٌ مِّنَ الۡاَوَّلِيۡنَۙ",
  "40": "وَثُلَّةٌ مِّنَ الۡاٰخِرِيۡنَؕ",
  "41": "وَاَصۡحٰبُ الشِّمَالِۙ مَاۤ اَصۡحٰبُ الشِّمَالِؕ",
  "42": "فِىۡ سَمُوۡمٍ وَّحَمِيۡمٍۙ",
  "43": "وَّظِلٍّ مِّنۡ يَّحۡمُوۡمٍۙ",
  "44": "لَّا بَارِدٍ وَّلَا كَرِيۡمٍ",
  "45": "اِنَّهُمۡ كَانُوۡا قَبۡلَ ذٰ لِكَ مُتۡرَفِيۡنَۚ ۖ",
  "46": "وَكَانُوۡا يُصِرُّوۡنَ عَلَى الۡحِنۡثِ الۡعَظِيۡمِ​ۚ",
  "47": "وَكَانُوۡا يَقُوۡلُوۡنَۙ اَٮِٕذَا مِتۡنَا وَكُنَّا تُرَابًا وَّعِظَامًا ءَاِنَّا لَمَبۡعُوۡثُوۡنَۙ",
  "48": "اَوَاٰبَآؤُنَا الۡاَوَّلُوۡنَ",
  "49": "قُلۡ اِنَّ الۡاَوَّلِيۡنَ وَالۡاٰخِرِيۡنَۙ",
  "50": "لَمَجۡمُوۡعُوۡنَۙ اِلٰى مِيۡقَاتِ يَوۡمٍ مَّعۡلُوۡمٍ",
  "51": "ثُمَّ اِنَّكُمۡ اَيُّهَا الضَّآلُّوۡنَ الۡمُكَذِّبُوۡنَۙ",
  "52": "لَاٰكِلُوۡنَ مِنۡ شَجَرٍ مِّنۡ زَقُّوۡمٍۙ",
  "53": "فَمٰلِــُٔوۡنَ مِنۡهَا الۡبُطُوۡنَ​ۚ",
  "54": "فَشٰرِبُوۡنَ عَلَيۡهِ مِنَ الۡحَمِيۡمِ​ۚ",
  "55": "فَشٰرِبُوۡنَ شُرۡبَ الۡهِيۡمِؕ",
  "56": "هٰذَا نُزُلُهُمۡ يَوۡمَ الدِّيۡنِؕ",
  "57": "نَحۡنُ خَلَقۡنٰكُمۡ فَلَوۡلَا تُصَدِّقُوۡنَ",
  "58": "اَفَرَءَيۡتُمۡ مَّا تُمۡنُوۡنَؕ",
  "59": "ءَاَنۡتُمۡ تَخۡلُقُوۡنَهٗۤ اَمۡ نَحۡنُ الۡخٰلِقُوۡنَ",
  "60": "نَحۡنُ قَدَّرۡنَا بَيۡنَكُمُ الۡمَوۡتَ وَمَا نَحۡنُ بِمَسۡبُوۡقِيۡنَۙ",
  "61": "عَلٰٓى اَنۡ نُّبَدِّلَ اَمۡثَالَـكُمۡ وَنُـنۡشِئَكُمۡ فِىۡ مَا لَا تَعۡلَمُوۡنَ ",
  "62": "وَلَـقَدۡ عَلِمۡتُمُ النَّشۡاَةَ الۡاُوۡلٰى فَلَوۡلَا تَذَكَّرُوۡنَ",
  "63": "اَفَرَءَيۡتُمۡ مَّا تَحۡرُثُوۡنَؕ",
  "64": "ءَاَنۡتُمۡ تَزۡرَعُوۡنَهٗۤ اَمۡ نَحۡنُ الزّٰرِعُوۡنَ",
  "65": "لَوۡ نَشَآءُ لَجَـعَلۡنٰهُ حُطَامًا فَظَلۡتُمۡ تَفَكَّهُوۡنَ",
  "66": "اِنَّا لَمُغۡرَمُوۡنَۙ",
  "67": "بَلۡ نَحۡنُ مَحۡرُوۡمُوۡنَ",
  "68": "اَفَرَءَيۡتُمُ الۡمَآءَ الَّذِىۡ تَشۡرَبُوۡنَؕ",
  "69": "ءَاَنۡـتُمۡ اَنۡزَلۡـتُمُوۡهُ مِنَ الۡمُزۡنِ اَمۡ نَحۡنُ الۡمُنۡزِلُوۡنَ ",
  "70": "لَوۡ نَشَآءُ جَعَلۡنٰهُ اُجَاجًا فَلَوۡلَا تَشۡكُرُوۡنَ",
  "71": "اَفَرَءَيۡتُمُ النَّارَ الَّتِىۡ تُوۡرُوۡنَؕ",
  "72": "ءَاَنۡتُمۡ اَنۡشَاۡتُمۡ شَجَرَتَهَاۤ اَمۡ نَحۡنُ الۡمُنۡشِـُٔـوۡنَ",
  "73": "نَحۡنُ جَعَلۡنٰهَا تَذۡكِرَةً وَّمَتَاعًا لِّلۡمُقۡوِيۡنَ​ۚ",
  "74": "فَسَبِّحۡ بِاسۡمِ رَبِّكَ الۡعَظِيۡمِ",
  "75": "فَلَاۤ اُقۡسِمُ بِمَوٰقِعِ النُّجُوۡمِۙ",
  "76": "وَاِنَّهٗ لَقَسَمٌ لَّوۡ تَعۡلَمُوۡنَ عَظِيۡمٌۙ",
  "77": "اِنَّهٗ لَـقُرۡاٰنٌ كَرِيۡمٌۙ",
  "78": "فِىۡ كِتٰبٍ مَّكۡنُوۡنٍۙ",
  "79": "لَّا يَمَسُّهٗۤ اِلَّا الۡمُطَهَّرُوۡنَؕ",
  "80": "تَنۡزِيۡلٌ مِّنۡ رَّبِّ الۡعٰلَمِيۡنَ",
  "81": "اَفَبِهٰذَا الۡحَـدِيۡثِ اَنۡتُمۡ مُّدۡهِنُوۡنَۙ",
  "82": "وَتَجۡعَلُوۡنَ رِزۡقَكُمۡ اَنَّكُمۡ تُكَذِّبُوۡنَ",
  "83": "فَلَوۡلَاۤ اِذَا بَلَغَتِ الۡحُـلۡقُوۡمَۙ",
  "84": "وَاَنۡتُمۡ حِيۡنَٮِٕذٍ تَـنۡظُرُوۡنَۙ",
  "85": "وَنَحۡنُ اَقۡرَبُ اِلَيۡهِ مِنۡكُمۡ وَلٰـكِنۡ لَّا تُبۡصِرُوۡنَ",
  "86": "فَلَوۡلَاۤ اِنۡ كُنۡتُمۡ غَيۡرَ مَدِيۡنِيۡنَۙ",
  "87": "تَرۡجِعُوۡنَهَاۤ اِنۡ كُنۡتُمۡ صٰدِقِيۡنَ",
  "88": "فَاَمَّاۤ اِنۡ كَانَ مِنَ الۡمُقَرَّبِيۡنَۙ",
  "89": "فَرَوۡحٌ وَّ رَيۡحَانٌۙ وَّجَنَّتُ نَعِيۡمٍ",
  "90": "وَاَمَّاۤ اِنۡ كَانَ مِنۡ اَصۡحٰبِ الۡيَمِيۡنِۙ",
  "91": "فَسَلٰمٌ لَّكَ مِنۡ اَصۡحٰبِ الۡيَمِيۡنِؕ",
  "92": "وَاَمَّاۤ اِنۡ كَانَ مِنَ الۡمُكَذِّبِيۡنَ الضَّآلِّيۡنَۙ",
  "93": "فَنُزُلٌ مِّنۡ حَمِيۡمٍۙ",
  "94": "وَّتَصۡلِيَةُ جَحِيۡمٍ",
  "95": "اِنَّ هٰذَا لَهُوَ حَقُّ الۡيَـقِيۡنِۚ",
  "96": "فَسَبِّحۡ بِاسۡمِ رَبِّكَ الۡعَظِيۡمِ"
}